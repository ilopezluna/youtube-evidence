<html t:type="layout" title="Listing videos"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd">

<ul class="video-list">
    <t:loop source="videos" value="video">
        <li>
            <t:videoembedder video="video"></t:videoembedder>
        </li>
    </t:loop>
</ul>

</html>